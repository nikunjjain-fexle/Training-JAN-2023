    <validationRules>
        <fullName>VR_Fields_Requiredness</fullName>
        <active>false</active>
        <description>Validation rule check fields for the required-ness for the data validation prospective while creation and updation of &quot;Flow Exception&quot; object record(s).</description>
        <errorConditionFormula>AND 
   (
   	ISBLANK(Originating_Flow_Name__c),
   	OR
   	(
   		ISPICKVAL(Element_Category__c, &apos;&apos;),
   		ISBLANK(Element_Name__c),
   		LEN(Exception_Message__c) = 0,
   		ISBLANK(Flow_Name__c),
   		ISBLANK(Logged_In_User__c),
   		ISBLANK(Object_API_Name__c),
   		ISBLANK(Record_ID__c)
   	)
)</errorConditionFormula>
        <errorMessage>Please have a value for &quot;Element Category, Element Name, Exception Message, Flow Name, Logged In User, Object API Name, Record ID&quot; fields when &quot;Originating Flow Name&quot; is blank.</errorMessage>
    </validationRules>
